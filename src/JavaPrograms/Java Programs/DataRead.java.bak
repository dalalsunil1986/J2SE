//DataRead.java
class DataRead 
{
	public static void main(String k[]) 
	{
		if(k.length==2)
		{
		System.out.println("No of command line args");
		}
		else
		{
			System.out.println("No of cmd line args="+k.length);
			for(int i=0;i<k.length;i++)
			{
				System.out.println(k[i]);
			
			}//for
		}//else
	}//main()
}//DataRead





	
